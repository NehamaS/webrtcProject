{
  "name": "service-infrastructure",
  "version": "1.0.191",
  "description": "Nest TypeScript starter repository",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest build && nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test:forceterminate": "nest build && jest --detectOpenHandles --runInBand --forceExit",
    "test": "jest --config ./jest.config.json",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "pack:dev": "npm run build; rm dist/package.json; rm dist/service-infrastructure*.tgz; cp package.json dist/package.json; cd dist; npm pack; cd ..",
    "pack:devwin": "if exist dist\\package.json del dist\\package.json && if exist dist\\service-infrastructure*.tgz del dist\\service-infrastructure*.tgz && copy package.json dist\\package.json && cd dist && npm pack && cd .."
  },
  "dependencies": {
    "@nestjs/axios": "0.0.2",
    "@nestjs/common": "^8.0.8",
    "@nestjs/config": "^1.0.0",
    "@nestjs/core": "^8.0.8",
    "@nestjs/platform-express": "^8.0.8",
    "@nestjs/platform-socket.io": "^8.0.8",
    "@nestjs/terminus": "^8.0.1",
    "@nestjs/testing": "8.2.4",
    "@nestjs/websockets": "^8.0.8",
    "@types/express": "^4.17.13",
    "@types/node": "^17.0.5",
    "axios": "^0.21.4",
    "better-queue": "^3.8.10",
    "check-disk-space": "^3.0.1",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.13.1",
    "express": "4.17.1",
    "flat": "5.0.2",
    "hot-shots": "^9.0.0",
    "js-yaml": "4.1.0",
    "lodash": "4.17.20",
    "logform": "^2.3.0",
    "mcu-discovery": "^1.0.162",
    "mcu-metrics": "^1.0.27",
    "mcu-persitence-service": "^1.0.60",
    "nestjs-hot-shots": "^2.0.6",
    "node-os-utils": "1.3.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.4.0",
    "thrift": "0.14.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.1.2",
    "@nestjs/schematics": "8.0.5",
    "@types/jest": "26.0.15",
    "@types/socket.io": "2.1.13",
    "@types/supertest": "2.0.10",
    "@types/thrift": "0.10.10",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.11.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.5.2",
    "prettier": "2.1.2",
    "supertest": "^4.0.2",
    "ts-jest": "26.4.1",
    "ts-loader": "^8.0.11",
    "ts-node": "^8.10.2",
    "tsconfig-paths": "^3.9.0",
    "typescript": "4.5.4"
  }
}
