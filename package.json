    "scripts": {
        "mocha": "mocha 'automation/capacity/jmeter/assertionTests/*.ts' --reporter mocha-junit-reporter ",
        "prebuild": "rimraf dist",
        "build": "nest build --tsc",
        "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
        "start": "nest start",
        "start:dev": "PLATFORM_TYPE=k8s CONF_PATH=k8s/helm/webrtc-gateway/config/pop/staging_us-east-1/config.json nest start --watch",
        "start:debug": "nest start --debug --watch",
        "start:prod": "node dist/main",
        "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
        "test": "npm run test:ut && npm run test:e2e",
        "test:watch": "jest --watch",
        "test:ut": "jest --config test/jest.config.json --detectOpenHandles --runInBand",
        "test:e2e": "jest --config test/e2e/jest-e2e.json --detectOpenHandles --runInBand",
        "test:ws": "jest --config test/simulators/ws/ws-jest.json",
        "test:login": "jest --config test/login/jest-login.json",
        "auto:run:local": "CONF_PATH=automation/component/config/config.json npm run start:prod",
        "auto:component": "LOG_LEVEL=INFO jest --verbose --config=automation/component/jest.config.js",
        "auto:e2e": "LOG_LEVEL=INFO jest --config=automation/e2e/jest.e2e.config.js",
        "auto:e2e:dev": "CPAAS_HOME=wss://1nkgbu7d6d.execute-api.us-east-1.amazonaws.com/webrtc-ws-apigw CPAAS_APP_URL=webrtc-dev.restcomm.com npm run auto:e2e",
        "auto:e2e:stg": "CPAAS_HOME=wss://yk11kg2xse.execute-api.us-east-1.amazonaws.com/webrtc-ws-apigw CPAAS_APP_URL=webrtc-staging.restcomm.com npm run auto:e2e",
        "docker:build": "docker build -f Dockerfile -t jenkins-k8s-harbor.il-labs.mavenir.com/k8s-cpaas/webrtc-gateway:0.0.1 .",
        "sync:repos": "rsync -avzh test/ ~/workspace/cpass/telestax/webrtc-gateway/test/; rsync -avzh src/ ~/workspace/cpass/telestax/webrtc-gateway/src/; rsync -avzh config/ ~/workspace/cpass/telestax/webrtc-gateway/config/"
    },
